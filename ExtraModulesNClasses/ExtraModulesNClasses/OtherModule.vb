Module OtherModule
    ''' <summary>
    ''' Returns the sum of two integer values 
    ''' </summary>
    ''' <param name="firstNumber%"></param>
    ''' <param name="secondNumber%"></param>
    ''' <returns>Int32</returns>
    Function SumOf(firstNumber%, secondNumber%) As Integer
        Return firstNumber + secondNumber
    End Function
End Module
